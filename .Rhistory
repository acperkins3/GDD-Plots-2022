PHP02 <- read.csv("~/gem-fact-field/Data2022/GEMS FACT DH X PHP02 2022 ARL Phenotypes.csv")
MBS5411 <- read.csv("~/gem-fact-field/Data2022/GEMS FACT DH X MBS5411 2022 ARL Phenotypes.csv")
